{
  "name": "muppet-kit",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "bin": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pkgroll --minify"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./tunnel": {
      "import": {
        "types": "./dist/tunnel.d.ts",
        "default": "./dist/tunnel.js"
      },
      "require": {
        "types": "./dist/tunnel.d.cts",
        "default": "./dist/tunnel.cjs"
      }
    }
  },
  "peerDependencies": {
    "@hono/node-server": "^1.14.1",
    "@muppet-kit/inspector": "workspace:*",
    "@muppet-kit/shared": "workspace:*",
    "@ngrok/ngrok": "^1.5.1",
    "c12": "^3.0.3",
    "commander": "^13.1.0",
    "execa": "^9.5.3",
    "hono": "^4.7.9",
    "untun": "^0.1.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "ai": "^4.3.13",
    "pkgroll": "^2.12.2"
  }
}